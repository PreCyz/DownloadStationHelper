            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ListView fx:id="torrentListView" layoutX="12.0" layoutY="126.0" prefHeight="233.0" prefWidth="578.0" style="-fx-border-style: solid; -fx-border-color: silver;" />
      <Pane layoutX="12.0" layoutY="37.0" prefHeight="84.0" prefWidth="381.0" style="-fx-border-style: solid; -fx-background-color: linear-gradient(to bottom right, silver, gainsboro); -fx-border-color: silver;">
         <children>
            <ComboBox fx:id="imdbComboBox" layoutX="7.0" layoutY="9.0" prefHeight="25.0" prefWidth="366.0">
