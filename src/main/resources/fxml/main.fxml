      <MenuBar prefHeight="29.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="%main.menu.config">
            <items>
              <MenuItem fx:id="applicationMenuItem" mnemonicParsing="false" text="%main.menu.config.application" />
